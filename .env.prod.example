# Telegram Bot Token
TELEGRAM_BOT_TOKEN="YOUR_TELEGRAM_BOT_TOKEN"

# MongoDB Connection String (with authentication)
MONGODB_URI="mongodb://username:password@mongo:27017/vinted-telegram-bot?authSource=admin"

# MongoDB Credentials (for Docker Compose)
MONGO_USERNAME=admin
MONGO_PASSWORD=secure_password

# MongoDB Express Credentials
MONGO_EXPRESS_USERNAME=admin
MONGO_EXPRESS_PASSWORD=secure_password

# The domain extension we should fetch data from the vinted API. (co.uk, fr, lt, etc)
VINTED_API_DOMAIN_EXTENSION="fr"

# Filter out 0 star reviews (0 = false, 1 = true)
ALGORITHM_FILTER_ZERO_STARS_PROFILES=1

# Max number of private chats a user can create
USER_MAX_PRIVATE_CHATS_DEFAULT=5

# Should the bot get the proxies from Webshare.io (0 = false, 1 = true)
USE_WEBSHARE=1

# Webshare.io API key
WEBSHARE_API_KEY="YOUR_API_KEY"

# Auto blacklist country codes (change as you wish, for example "uk,fr,us" to blacklist UK, FR and US)
BLACKLISTED_COUNTRIES_CODES="uk"

# Concurrent requests to Vinted API
ALGORITHM_CONCURRENT_REQUESTS=5

# Polling interval in seconds
POLLING_INTERVAL=60

# Admin user IDs (comma-separated list of Telegram user IDs)
ADMIN_USER_IDS="YOUR_TELEGRAM_USER_ID" 